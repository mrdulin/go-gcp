{
  "name": "angular-route-demos",
  "version": "1.0.0",
  "description": "angular-route demos with browserify",
  "main": "",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "./node_modules/.bin/mkdirp dist && browserify app/app.js -o dist/bundle.js -p [parcelify -o dist/bundle.css]",
    "watch": "./node_modules/.bin/watchify app/app.js -o dist/bundle.js -p [parcelify -wo dist/bundle.css] --debug --verbose",
    "serve": "./node_modules/.bin/beefy app/app.js:dist/bundle.js 3000 --live --open --cwd ./app"
  },
  "style": [
    "./node_modules/bootstrap/dist/css/bootstrap.theme.css",
    "./node_modules/bootstrap/dist/css/bootstrap.css",
    "app/**/*.css"
  ],
  "keywords": [
    "angular",
    "angular-route",
    "browserify",
    "watchify"
  ],
  "author": "dulin",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.5",
    "angular-route": "^1.5.5",
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.3"
  },
  "devDependencies": {
    "beefy": "^2.1.6",
    "browserify": "^13.0.1",
    "mkdirp": "^0.5.1",
    "parcelify": "^2.1.0",
    "watchify": "^3.7.0"
  }
}
